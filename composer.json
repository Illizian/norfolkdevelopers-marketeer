{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "atymic/twitter": "^3.1",
        "ebess/advanced-nova-media-library": "^3.6",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illizian/nova-carbon-modifier-field": "^0.1.0",
        "illizian/nova-emoji-field-container": "^0.1.0",
        "illizian/nova-resource-calendar": "*",
        "illizian/nova-rrule-field": "*",
        "illizian/nova-suggest-field-container": "^0.1.0",
        "laravel-notification-channels/discord": "^1.1",
        "laravel-notification-channels/twitter": "^5.0",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^1.6",
        "laravel/nova": "^3.12",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^2.0",
        "rlanvin/php-rrule": "^2.2",
        "sentry/sentry-laravel": "^2.1",
        "spatie/laravel-permission": "^3.18",
        "twilio/sdk": "^6.35",
        "vyuldashev/nova-permission": "^2.9"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.3",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaResourceCalendar"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaRruleField"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "@php artisan nova:publish"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
